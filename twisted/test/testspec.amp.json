{"commands" : [{"name"      : "Sum",
                "arguments" : [["a", "Integer"],
                               ["b", "Integer"]],
                "response"  : [["total", "Integer"]]
                },

               {"name"      : "Divide",
                "arguments" : [["numerator", "Float"],
                               ["denominator", "Float"]],
                "response"  : [["result", "Float"]],
            
                "errors"    : [["ZERO_DIVISION", "ZeroDivisionError"]]
                },

               {"name"      : "SumList",
                "arguments" : [["args", {"type" : "ListOf", "of" : "Integer"}],
                               ["basevalue", {"type" : "Integer",
                                              "optional" : true}]
                              ],
                "response"  : [["total", "Integer"]]
                },

               {"name"      : "SumPairs",
                "arguments" : [["args", {"type" : "TupleList",
                                         "of" : ["Integer", "Integer"]}]],
                "response" : [["total", "Integer"]]
                }
               ]
}
